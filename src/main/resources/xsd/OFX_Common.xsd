      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:sequence>
          <xsd:element name="FIRSTNAME" type="ofx:GenericNameType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="MIDDLENAME" type="ofx:GenericNameType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="LASTNAME" type="ofx:GenericNameType" minOccurs="1" maxOccurs="1"/>
          <xsd:sequence minOccurs="0" maxOccurs="1">
            <xsd:element name="BUSNAME" type="ofx:GenericNameType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="URL" type="ofx:UrlType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="SIC" type="ofx:StandardIndustryCodeType" minOccurs="0" maxOccurs="1"/>
          </xsd:sequence>		
        </xsd:sequence>
        <xsd:sequence>
          <xsd:element name="BUSNAME" type="ofx:GenericNameType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="URL" type="ofx:UrlType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="SIC" type="ofx:StandardIndustryCodeType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>		
      </xsd:choice>
    </xsd:sequence>
