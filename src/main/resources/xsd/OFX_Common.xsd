		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="collapse"/>
			<xsd:enumeration value="OPAQUE"/>
			<xsd:enumeration value="URL"/>
			<xsd:enumeration value="FORMURL"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ImageDelayType">
