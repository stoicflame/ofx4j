      </xsd:annotation>
      <xsd:restriction base="xsd:string">
          <xsd:whiteSpace value="collapse"/>
          <xsd:enumeration value="INDIVIDUAL"/>
          <xsd:enumeration value="JOINT"/>
          <xsd:enumeration value="CUSTODIAL"/>
          <xsd:enumeration value="TRUST"/>
          <xsd:enumeration value="OTHER"/>
      </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AccountHolderType">
      <xsd:annotation>
          <xsd:documentation>
           The OFX element "PRIMARYHOLDER" and "SECONDARYHOLDER" is of type "AccountHolderType"
          </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
          <xsd:element name="FIRSTNAME" type="ofx:GenericNameType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="MIDDLENAME" type="ofx:GenericNameType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="LASTNAME" type="ofx:GenericNameType" minOccurs="1" maxOccurs="1"/>
          <xsd:sequence minOccurs="1" maxOccurs="1">
              <xsd:element name="ADDR1" type="ofx:AddressType" minOccurs="1" maxOccurs="1"/>
              <xsd:sequence minOccurs="0" maxOccurs="1">
                  <xsd:element name="ADDR2" type="ofx:AddressType" minOccurs="1" maxOccurs="1"/>
                  <xsd:element name="ADDR3" type="ofx:AddressType" minOccurs="0" maxOccurs="1"/>
              </xsd:sequence>		
          </xsd:sequence>
          <xsd:element name="CITY" type="ofx:AddressType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="STATE" type="ofx:StateType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="POSTALCODE" type="ofx:ZipType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="COUNTRY" type="ofx:CountryType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="DAYPHONE" type="ofx:PhoneType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="EVEPHONE" type="ofx:PhoneType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="EMAIL" type="ofx:ShortMessageType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="HOLDERTYPE" type="ofx:HolderTypeEnum" minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="HolderInfoType">
      <xsd:annotation>
          <xsd:documentation>
           The OFX element "HOLDERINFO" is of type "HolderInfoType"
          </xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
          <xsd:element name="PRIMARYHOLDER" type="ofx:AccountHolderType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="SECONDARYHOLDER" type="ofx:AccountHolderType" minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>		
  </xsd:complexType>
  <xsd:simpleType name="AccountClassificationEnum">
      <xsd:annotation>
          <xsd:documentation>
           The OFX element "ACCTCLASSIFICATION" is of type "AccountClassificationEnum"
          </xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
          <xsd:whiteSpace value="collapse"/>
          <xsd:enumeration value="PERSONAL"/>
          <xsd:enumeration value="BUSINESS"/>
          <xsd:enumeration value="CORPORATE"/>
          <xsd:enumeration value="OTHER"/>
      </xsd:restriction>
