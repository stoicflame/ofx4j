      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:sequence>
          <xsd:element name="FIRSTNAME" type="ofx:EncryptedContentA256" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="MIDDLENAME" type="ofx:EncryptedContentA256" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="LASTNAME" type="ofx:EncryptedContentA256" minOccurs="1" maxOccurs="1"/>
          <xsd:sequence minOccurs="0" maxOccurs="1">
            <xsd:element name="BUSNAME" type="ofx:EncryptedContentA256" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="URL" type="ofx:EncryptedContentA512" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="SIC" type="ofx:EncryptedContentA256" minOccurs="0" maxOccurs="1"/>
          </xsd:sequence>		
        </xsd:sequence>
        <xsd:sequence>
          <xsd:element name="BUSNAME" type="ofx:EncryptedContentA256" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="URL" type="ofx:EncryptedContentA512" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="SIC" type="ofx:EncryptedContentA256" minOccurs="0" maxOccurs="1"/>
