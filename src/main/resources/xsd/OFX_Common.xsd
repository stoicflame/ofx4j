					<xsd:element name="SUPTXDL" type="ofx:BooleanType"/>
					<xsd:element name="XFERSRC" type="ofx:BooleanType"/>
					<xsd:element name="XFERDEST" type="ofx:BooleanType"/>
					<xsd:element name="LOANACCTTYPE" type="ofx:LoanAccountEnum" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="MATURITYDATE" type="ofx:DateTimeType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="MATURITYAMT" type="ofx:AmountType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="MINBALREQ" type="ofx:AmountType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="ACCTCLASSIFICATION" type="ofx:AccountClassificationEnum" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="OVERDRAFTLIMIT" type="ofx:AmountType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="SVCSTATUS" type="ofx:ServiceStatusEnum"/>
