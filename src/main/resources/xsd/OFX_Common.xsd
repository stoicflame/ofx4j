        </xsd:sequence>		
      </xsd:sequence>
      <xsd:element name="CITY" type="ofx:EncryptedContentA256" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="STATE" type="ofx:EncryptedContentA256" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="POSTALCODE" type="ofx:EncryptedContentA256" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="COUNTRY" type="ofx:EncryptedContentA256" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="PHONE" type="ofx:EncryptedContentA256" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ALTPHONE" type="ofx:EncryptedContentA256" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="EMAIL" type="ofx:EncryptedContentA512" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ALTEMAIL" type="ofx:EncryptedContentA512" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
