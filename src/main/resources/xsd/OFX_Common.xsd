			<xsd:element name="LASTPMTINFO" type="ofx:LastPmtInfoType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="LOANDETAIL" type="ofx:LoanDetailType" minOccurs="0" maxOccurs="1"/>
