    <xsd:annotation>
      <xsd:documentation>
       The OFX element OTHERACCTINFO is of type "OtherAccountInfo"
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="BANKID" type="ofx:BankIdType" minOccurs="0"/>
      <xsd:element name="ACCTID" type="ofx:AccountIdType"/>
      <xsd:element name="ACCTUSAGE" type="ofx:AccountUsageType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="EncryptedContentA256">
