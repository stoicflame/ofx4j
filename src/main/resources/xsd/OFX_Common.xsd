  <xsd:simpleType name="EncryptionInfoA10000">
    <xsd:annotation>
      <xsd:documentation>
        The OFX elements "ENCRYPTION" is of type "EncryptionInfoA10000" 
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="10000"/>
    </xsd:restriction>
