      <xsd:documentation>
        The OFX element "USERINFORQ" is of type "UserInfoRequest"
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DTUSERUP" type="ofx:DateTimeType"/>
