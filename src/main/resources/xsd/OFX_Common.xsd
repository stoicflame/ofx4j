			<xsd:element name="PASTDUEAMT" type="ofx:AmountType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="LATEFEEAMT" type="ofx:AmountType" minOccurs="0" maxOccurs="1"/>
