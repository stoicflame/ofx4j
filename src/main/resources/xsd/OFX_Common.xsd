      <xsd:element name="ALTPHONE" type="ofx:PhoneType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="EMAIL" type="ofx:ShortMessageType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ALTEMAIL" type="ofx:ShortMessageType" minOccurs="0" maxOccurs="1"/>
